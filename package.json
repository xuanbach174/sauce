{
  "name": "nfs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js",
    "test:chromium": "set BROWSER=chromium&& cucumber-js --profile chromium",
    "test:firefox": "set BROWSER=firefox&& cucumber-js --profile firefox",
    "test:webkit": "set BROWSER=webkit&& cucumber-js --profile webkit",
    "test:all": "concurrently \"npm run test:chromium\" \"npm run test:firefox\" \"npm run test:webkit\"",
    "test:all-browsers": "npm run test:chromium && npm run test:firefox && npm run test:webkit",
    "test:parallel": "cucumber-js --parallel 3",
    "test:parallel:report": "npm run test:parallel || start reports/cucumber-report.html",
    "test:report": "node scripts/generate-report.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@cucumber/cucumber": "^12.6.0",
    "@playwright/test": "^1.58.0",
    "concurrently": "^9.2.1",
    "playwright": "^1.58.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}